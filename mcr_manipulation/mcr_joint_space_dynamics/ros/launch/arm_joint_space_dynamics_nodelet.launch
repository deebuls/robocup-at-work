<?xml version="1.0"?>
<launch>
  <arg name="input_topic" default="/joint_states" />
  <arg name="node_name" default="mcr_joint_space_dynamics" />
  <arg name="manager" default="nodelet_mcr_joint_space_dynamics" />
  <arg name="type" default="load" />
  
  <node pkg="nodelet" type="nodelet" name="$(arg node_name)" ns="mcr_manipulation" args="$(arg type) mcr_joint_space_dynamics/ArmJointSpaceDynamics $(arg manager)" output="screen" >
    <remap from="~/joint_states" to="$(arg input_topic)" />
    <rosparam command="load" file="$(find mcr_joint_space_dynamics)/ros/config/arm_joint_space_dynamics.yaml" />
  </node>
  
</launch>
