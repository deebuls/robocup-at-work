<?xml version="1.0"?>

<package>

  <name>mcr_blob_detection</name>
  <version>0.1.0</version>
  <description>Blob Detection Package - Uses cvblobs library to detect all blobs satisfying the minimum and maximum area provided</description>

  <maintainer email="mashoksc@gmail.com">Ashok Meenakshi Sundaram</maintainer>

  <license>GPLv3</license>

  <author email="mashoksc@gmail.com">Ashok Meenakshi Sundaram</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cv_bridge</build_depend>
  <build_depend>cvblobs</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>std_msgs</build_depend>
  <build_depend>mcr_perception_msgs</build_depend>

  <run_depend>cv_bridge</run_depend>
  <run_depend>cvblobs</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>std_msgs</run_depend>
  <run_depend>mcr_perception_msgs</run_depend>

  <test_depend>roslaunch</test_depend>

</package>
